{
    "name": "slim/slim-app",
    "description": "A Slim Framework  application for video tutorials development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.1",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
	"scripts": {
        "start": "php -S 0.0.0.0:8080 -t public index.php",
        "test": "phpunit"
	}

}
